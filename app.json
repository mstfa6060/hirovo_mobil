{
  "expo": {
    "name": "Hirovo",
    "slug": "hirovo-mobil",
    "owner": "mstfa6060",
    "scheme": "hirovo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.madentechnology.hirovomobilapp",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "https",
            "host": "hirovo.page.link"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": {
            "scheme": "hirovo",
            "host": "*"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "ios": {
      "bundleIdentifier": "com.madentechnology.hirovomobilapp",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      },
      "entitlements": {
        "aps-environment": "development",
        "com.apple.security.application-groups": [
          "group.com.madentechnology.hirovomobilapp.onesignal"
        ]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "oneSignalAppId": "d22118fa-f46c-496a-8ac9-34f2a8de29a1",
      "eas": {
        "projectId": "2122db4d-ba6e-4881-9d43-a8ee5d32a8e3"
      }
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ],
      "expo-localization"
    ]
  }
}